<h2 style="color:crimson;">Listado de Productos</h2>

<% if(productos.length > 0){ %> 
    <div class="table-responsive">
        <table class="table table-dark">
            <tr style="color: yellow;"> <th>Id</th> <th>Time</th> <th>Nombre</th> <th>Descripcion</th> <th>Codigo</th> <th>Foto</th> <th>Precio</th> <th>Stock</th> </tr>
            <!-- filas -->
            <% for (const producto of productos) {%> 
            <tr> 
                <td><%=producto.Id %> </td>
                <td><%=producto.TimeStamp %> </td>
                <td><%=producto.Nombre %> </td>
                <td><%=producto.Descripcion %> </td>
                <td><%=producto.Codigo %> </td>
                <td><%=producto.Foto %> </td>
                <td><%=producto.Precio %> </td>
                <td><%=producto.Stock %> </td>
            </tr>
            <% } %> 
            <!-- Fin filas -->
        </table>
    </div>
<% } else { %> 
    <h3 class="alert alert-danger">No se encontraron dato</h3>
<% } %> 
